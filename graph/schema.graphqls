"The `Upload` scalar type represents a multipart file upload."
scalar Upload

type File {
    id: Int!
    name: String!
    content: String!
    contentType: String!
}

input UploadFile {
    image: Upload!
}

"The `Query` type, represents all of the entry points into our object graph."
type Query {
    getHistogram(file: UploadFile!): String!
}

